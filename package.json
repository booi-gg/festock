{
  "name": "festock",
  "version": "1.0.8",
  "description": "Scaffold a full frontend stack with Vite, React Router v7, TanStack Query, Tailwind CSS 4, and shadcn/ui",
  "type": "module",
  "bin": {
    "festock": "bin/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --outDir bin --clean && shx chmod +x bin/index.js",
    "publish": "npm run build && npm publish"
  },
  "keywords": [
    "vite",
    "react",
    "cli",
    "frontend-stack",
    "boilerplate",
    "react-router",
    "tanstack-query",
    "tailwind",
    "shadcn"
  ],
  "files": [
    "bin",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/booi-gg/festock.git"
  },
  "homepage": "https://github.com/booi-gg/festock#readme",
  "author": "booi.gg",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@types/node": "^24.0.15",
    "@types/prompts": "^2.4.9",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "prompts": "^2.4.2",
    "strip-json-comments": "^5.0.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
